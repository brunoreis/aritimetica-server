{
  "name": "aritimetica-server",
  "version": "0.0.0",
  "license": "MIT",
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --transpile-only --no-notify api/index.ts",
    "prisma:seed": "npx prisma migrate reset",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "apollo-server": "3.12.0",
    "graphql": "16.6.0",
    "nexus": "1.3.0"
  },
  "devDependencies": {
    "prisma": "^4.12.0",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.0.4"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "printWidth": 80,
    "trailingComma": "all",
    "semi": false
  }
}
